{
  "name": "ionic-space-shooter",
  "version": "0.0.1",
  "author": "Nicolas Fazio",
  "homepage": "http://nicolasfazio.ch/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "electron:prod": "ionic cordova build browser && electron .",
    "electron:build": "ionic cordova build browser && electron-packager . --overwrite",
    "clear:osx": "rm -rf *-darwin-x64; rm -rf ./platforms/*-mas-x64",
    "clear:win": "rm -rf *-win32-ia32; rm -rf ./platforms/*-win32-x64",
    "clear:nix": "rm -rf *-linux-ia32; rm -rf ./platforms/*-linux-x64",
    "pack:osx": "npm run clear:osx && electron-packager ./ --platform=darwin,mas --arch=x64 --asar --out=platforms ",
    "pack:win": "npm run clear:win && electron-packager ./ --platform=win32 --arch=ia32,x64 --asar --out=platforms ",
    "pack:nix": "npm run clear:nix && electron-packager ./ --platform=linux --arch=ia32,x64 --asar --out=platforms ",
    "deploy":"firebase deploy"
  },
  "dependencies": {
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@ionic-native/core": "3.12.1",
    "@ionic-native/native-audio": "^4.1.0",
    "@ionic-native/splash-screen": "3.12.1",
    "@ionic-native/status-bar": "3.12.1",
    "@ionic/storage": "2.0.1",
    "@ngrx/effects": "^4.0.2",
    "@ngrx/store": "^4.0.2",
    "@ngrx/store-devtools": "^4.0.0",
    "angularfire2": "^4.0.0-rc.1",
    "firebase": "^4.2.0",
    "ionic-angular": "^3.6.0",
    "ionicons": "3.0.0",
    "rxjs": "5.4.0",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^2.1.3",
    "@ionic/cli-plugin-cordova": "1.6.2",
    "@ionic/cli-plugin-ionic-angular": "1.4.1",
    "electron": "^1.6.11",
    "electron-packager": "^8.7.2",
    "ionic": "3.7.0",
    "ngrx-store-freeze": "^0.1.9",
    "typescript": "2.3.4"
  },
  "description": "An Ionic project",
  "main": "electron.js"
}
